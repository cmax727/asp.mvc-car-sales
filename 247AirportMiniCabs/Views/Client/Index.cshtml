<div class="top-notification"></div>
@{
    var clientAllConfirmedJobs = Model.Jobs.Count(j => j.ClientToken == you.Token);
}
<script>    var clientAllConfirmedJobs = '@clientAllConfirmedJobs';</script>
@if(clientAllConfirmedJobs > 0 && clientAllConfirmedJobs % 10 == 1)
{
    <script>
        var remaining = (clientAllConfirmedJobs / 10);
        remaining = (remaining + '').split('.')[1];
        remaining = 10 - parseInt(remaining);

        remaining && $('.top-notification').html('You are <span class="roshan">' + remaining + '</span> booking away from your <span class="roshan">10%</span> bonus!').slideDown();
    </script>
}
else if(clientAllConfirmedJobs > 0 && clientAllConfirmedJobs % 10 == 0)
{
    <script>        $('.top-notification').html('Congratulation Sir, you are given 10% discount for your next booking.<br />To get <span class="roshan">10%</span> discount, please use this token: <input type=text value="@Model.GetDiscountToken("10")" style="margin-top: 15px;"><br /><br />This token is valid till: @Model.GetDiscountValidTill()').slideDown();</script>
}
else if(clientAllConfirmedJobs < 10 && clientAllConfirmedJobs >= 0)
{
    if(clientAllConfirmedJobs == 0) 
     {
            <script>                $('.top-notification').html('Congratulation Sir, you are given 5% discount for your first booking.<br />To get <span class="roshan">5%</span> discount, please use this token: <input type=text value="@Model.GetDiscountToken("5")" style="margin-top: 15px;"><br /><br />This token is valid till: @Model.GetDiscountValidTill()').slideDown();</script>
     }
     else 
     {
        <script>
            var remaining = 10 - clientAllConfirmedJobs;
            remaining && $('.top-notification').html('You are <span class="roshan">' + remaining + '</span> booking away from your <span class="roshan">10%</span> bonus!').slideDown();
        </script>
     }
}

